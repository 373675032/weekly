<!-- This is intentionally inlined to avoid FOUC -->
<script is:inline>
  document.addEventListener('DOMContentLoaded', function () {
    const observer = lozad('.lozad',{
     loaded: function(el) {
           el.alt = el.getAttribute('data-alt')
        }
    });
    observer.observe();

    const heti = new Heti('.heti');
    heti.autoSpacing();
  });
</script>

